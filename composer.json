{
    "name": "y-martini/qr-code-reader-nova-field",
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "laravel/nova": "~4.0"
    },
    "autoload": {
        "psr-4": {
            "YMartini\\Nova\\Fields\\QRCodeReader\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "YMartini\\Nova\\Fields\\QRCodeReader\\ServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        }
    ]
}
